class Solution {
    static int dataTypeSize(String str) {
        if(str.equals("Character") || str.equals("Boolean")){
            return 1;
        }
        else if(str.equals("Integer") || str.equals("Float")){
            return 4;
        }
        else if(str.equals("Double") || str.equals("Long")){
            return 8;
        }
        else{
            return 1;
        }
        
    }
}
